 <script src="http://cdn.jquerytools.org/1.2.7/full/jquery.tools.min.js"></script>
<script src='http://datejs.googlecode.com/svn/trunk/build/date.js'></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/client.js"></script>
<link rel="stylesheet" href="/client.css"/>
</head>
<body>
<h1>Admin - Todas las conversaciones</h1>
<b>Server: </b><%= server %>
<br/>
<% if(user) { %>
<b>User: </b> <label id="user"><%= user %></label> <a href="/logout" style='color:blue;text-decoration: underline'> Log out</a>
<h4 style='text-align:center'>Comparti este link en las redes sociales: <br/><b>http://chat123.com.ar/nick/<%= user %></b></h4>
<% } %>


<div id="ask" class="bordered" style="display: none;">
    Name: <input type="text" id="name"/> <a href="#"><span class="join">Join!</span></a>
</div>
<div id="channel" class="bordered" style="display: none;">
<h4>Server: <%= server %> - conectado como  <%= user %></h4>

 <ul class="tabs">
    
  </ul>

    <div id="msgs">
        <div class="panes">
    
  </div>

        <ul>
            <li class="message" style="display: none">
                <span class="user"></span><span class="message"></span>
                <span class="time"></span>
            </li>
            <li class="control" style="display: none">
                <span class="user"></span>&nbsp;<span class="message"></span>
                <span class="time"></span>
            </li>
        </ul>
    </div>
    <div id="input">
        <form><input type="text" data-chat-to="<%= nick %>" id="message"/></form>
    </div>
</div>


<!-- the tabs -->
 
 
  <!-- tab "panes" -->
  
  <script>$("ul.tabs").tabs("div.panes > div");</script>
</body>
</html>